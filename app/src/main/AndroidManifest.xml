<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.elf.elfstudent">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activities.InstitutePage"
            android:exported="true"/>
        <activity android:name=".Activities.HomeActivity"
            android:exported="true"/>
        <activity android:name=".Activities.RegisterActivity"
            android:exported="true"/>

        <activity android:name=".Activities.TestPageActivity"
            android:exported="true"/>
        <activity android:name=".Activities.LoginActivity"
            android:exported="true"/>
        <activity android:name=".Activities.BrowseTestActivity"
            android:exported="true"/>
        <activity android:name=".Activities.ReportActivity"
            android:exported="true"/>
        <activity android:name=".Activities.TestCompletedActivity"
            android:exported="true"/>
        <activity android:name=".Activities.TestReportsActivity"
            android:exported="true"/>
        <activity android:name=".Activities.SingleTestReportActivity"
            android:exported="true"/>

        <activity android:name=".Activities.SubjectViewActivity"/>



    </application>

</manifest>