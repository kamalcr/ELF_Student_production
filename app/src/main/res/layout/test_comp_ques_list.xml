<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@color/white"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_below="@+id/textView11"
        android:id="@+id/frame_root_qlist_frag"
        android:layout_marginTop="16dp"
        android:layout_height="match_parent">

        <include layout="@layout/loading_list_white"/>

    </FrameLayout>

    <com.elf.elfstudent.CustomUI.HelviticaLight
        android:text="Question List"
        android:textColor="@color/black_viewpager"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="43dp"
        android:id="@+id/textView10"
        android:textAppearance="@style/TextAppearance.AppCompat.Title"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <com.elf.elfstudent.CustomUI.HelviticaLight
        android:text="See your Answers"
        android:textColor="@color/black_900"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:id="@+id/textView11"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:layout_below="@+id/textView10"
        android:layout_centerHorizontal="true" />

</RelativeLayout>