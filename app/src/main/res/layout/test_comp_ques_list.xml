<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@color/black_viewpager"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <com.elf.elfstudent.CustomUI.HelviticaLight
        android:text="Question List"
        android:textColor="@color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="51dp"
        android:id="@+id/textView10"
        android:textAppearance="@style/TextAppearance.AppCompat.Title" />

    <com.elf.elfstudent.CustomUI.HelviticaLight
        android:text="See your Answers"
        android:textColor="@color/white_light"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView10"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:layout_marginTop="11dp"
        android:id="@+id/textView11"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_below="@id/textView11"
        android:id="@+id/frame_root_qlist_frag"
        android:layout_height="match_parent">

        <include layout="@layout/loading_list"/>

    </FrameLayout>

</RelativeLayout>