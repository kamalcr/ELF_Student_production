<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"


    android:layout_width="match_parent"
    android:background="@color/white"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    android:layout_height="wrap_content">


    <com.elf.elfstudent.CustomUI.QucikSand
        android:layout_width="wrap_content"
        android:id="@+id/list_home_title"
        android:textStyle="bold"
        android:textColor="@color/black_900"
        android:layout_marginTop="8dp"
        android:text="@string/subject_wise_analysis"
        android:layout_centerHorizontal="true"
        android:textSize="@dimen/home_sub_mtop"
        android:layout_height="wrap_content" />

    <android.support.v7.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/home_list_rv"
        android:layout_below="@+id/list_home_title"
        android:layout_marginBottom="8dp"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        android:layout_marginTop="16dp"/>

</RelativeLayout>